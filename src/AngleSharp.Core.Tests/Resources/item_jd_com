    ;(function(doc, win) {
        var insertScript = function(url) {
            var head = document.getElementsByTagName('head')[0];
            var script = document.createElement('script');
            script.setAttribute('type', 'text/javascript');
            script.setAttribute('src', url);
            head.appendChild(script);
        };
        win._getSkuPrice = function (r) {
            //var el = doc.getElementById('J_price_trigger');
			var jp = $('#summary-price').find('strong');
			var mp = $('#summary-price').find('em s');
			var jdis = $('#summary-price').find('span');

            if ( r && r.length > 0 & parseFloat(r[0].p) > 0) {
                jp.html('￥' + r[0].p)
				mp.html('￥' + r[0].m);
                jdis.html('['+(Math.ceil(r[0].p/r[0].m * 100) / 10).toFixed(1) + '折]');
            } else {
                jp.innerHTML = '暂无报价';
            }
            win._getSkuPrice = null;
        };
        insertScript('http://p.3.cn/prices/mgets?type=1&callback=_getSkuPrice&skuIds=J_11312278');
    })(document, window);
</script>
